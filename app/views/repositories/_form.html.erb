<div class="wrapper wrapper-content animated fadeInRight">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h2>New Repository</h2>
                            <div class="ibox-tools">
                                <a class="collapse-link">
                                    <i class="fa fa-chevron-up"></i>
                                </a>
                                <a class="close-link">
                                    <i class="fa fa-times"></i>
                                </a>
                            </div>
                        </div>
                <div class="ibox-content">
                <%= form_for(@repository) do |f| %>
    <% if @repository.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@repository.errors.count, "error") %> prohibited this repository from being saved:</h2>

      <ul>
      <% @repository.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group row">
    <%= f.label :repo_id, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.number_field :repo_id, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :name, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :name, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :full_name, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :full_name, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :github_profile_nickname, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :github_profile_nickname, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :html_url, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :html_url, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :description, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_area :description, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :git_url, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :git_url, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :ssh_url, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :ssh_url, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :clone_url, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :clone_url, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :watchers_count, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.number_field :watchers_count, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :language, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :language, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :has_issues, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.check_box :has_issues, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :has_downloads, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.check_box :has_downloads, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :has_wiki, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.check_box :has_wiki, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :forks_count, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.number_field :forks_count, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :open_issues_count, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.number_field :open_issues_count, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :open_issues, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.check_box :open_issues, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :watchers, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.number_field :watchers, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :codezip, "Upload Your Code Here", class: 'col-sm-2 control-label' %>
     <div class="col-sm-10">
    <%= f.file_field :codezip, class: 'form-control' %>
  </div>
  <div class="form-group row">
    <%= f.label :amount, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.number_field :amount, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
  <%= f.label :tag_list, "Tags (separated by commas)", class: 'col-sm-2 control-label' %>
  <div class="col-sm-10">
  <%= f.text_field :tag_list, class: 'form-control' %>
  </div>
  </div>
  <div class="form-group row">
    <%= f.label :fork, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.check_box :fork, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :crated_at, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.datetime_select :crated_at, class: 'form-control' %><br />
    </div>
  </div>
    <br>
    <br>
  <div class="actions">
    <%= f.submit class: 'btn btn-primary' %>
  </div>
<% end %>
</div>