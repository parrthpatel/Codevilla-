<p id="notice"><%= notice %></p>

<h1>Listing Repositories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Github profile nickname</th>
      <th>Full name</th>
      <th>Html url</th>
      <th>Description</th>
      <th>Crated at</th>
      <th>Git url</th>
      <th>Svn url</th>
      <th>Watchers count</th>
      <th>Language</th>
      <th>Has issues</th>
      <th>Has wiki</th>
      <th>Forks count</th>
      <th>Open issues count</th>
      <th>Open issues</th>
      <th>Watchers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @repositories.each do |repository| %>
      <tr>
        <td><%= repository.name %></td>
        <td><%= repository.github_profile_nickname %></td>
        <td><%= repository.full_name %></td>
        <td><%= repository.html_url %></td>
        <td><%= repository.description %></td>
        <td><%= repository.crated_at %></td>
        <td><%= repository.git_url %></td>
        <td><%= repository.svn_url %></td>
        <td><%= repository.watchers_count %></td>
        <td><%= repository.language %></td>
        <td><%= repository.has_issues %></td>
        <td><%= repository.has_wiki %></td>
        <td><%= repository.forks_count %></td>
        <td><%= repository.open_issues_count %></td>
        <td><%= repository.open_issues %></td>
        <td><%= repository.watchers %></td>
        <td><%= link_to 'Show', repository %></td>
        <td><%= link_to 'Edit', edit_repository_path(repository) %></td>
        <td><%= link_to 'Destroy', repository, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Repository', new_repository_path %>
