<div class="wrapper wrapper-content animated fadeInRight">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Editing Your Profile Details</h5>
                            <div class="ibox-tools">
                                <a class="collapse-link">
                                    <i class="fa fa-chevron-up"></i>
                                </a>
                                <a class="close-link">
                                    <i class="fa fa-times"></i>
                                </a>
                            </div>
                        </div>
                        <div class="ibox-content">
<%= form_for @github_profile, class: 'form-horizontal' do |f| %>
  <% if @github_profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@github_profile.errors.count, "error") %> prohibited this github_profile from being saved:</h2>

      <ul>
      <% @github_profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div>
  <div class="field">
    <%= f.label :email, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :email, class: 'form-control' %>
    </div>
  </div>
  <div class="field">
    <%= f.label :name, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :name, class: 'form-control' %>
    </div> 
  </div>
  <div class="field">
    <%= f.label :location, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :location, class: 'form-control' %>
    </div>
  </div>
  <div class="field">
    <%= f.label :github_link, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :github_link, class: 'form-control' %>
    </div>
  </div>
  <div class="field">
    <%= f.label :blog_link, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_field :blog_link, class: 'form-control' %>
    </div>
  </div>
  <div class="field">
    <%= f.label :bio, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= f.text_area :bio, class: 'form-control', rows: 16 %>
    </div>
  </div>
  <div class="center">
  <div class="actions">
    <%= f.submit value: 'Update Profile', :class => 'btn btn-primary center-block' %>
  </div></div>
<% end %>
<%= link_to 'Back', welcome_profile_path, class: 'btn btn-white' %>
</div>
</div>
</div>
</div>
</div>