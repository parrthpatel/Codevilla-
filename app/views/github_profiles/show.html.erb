<div class="wrapper wrapper-content animated fadeInRight">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Your Github Profile Details</h5>
                            <div class="ibox-tools">
                                <a class="collapse-link">
                                    <i class="fa fa-chevron-up"></i>
                                </a>
                                <a class="close-link">
                                    <i class="fa fa-times"></i>
                                </a>
                            </div>
                        </div>

<div class="ibox-content">
<p id="notice"><%= notice %></p>
<table class="table table-hover">
<tr>
  <td><strong>Nickname:</strong></td>
  <td><%= @github_profile.nickname %></td>
  </tr>
<tr>
  <td><strong>Email:</strong></td>
  <td><%= @github_profile.email %></td>
</tr>
<tr>
  <td><strong>Name:</strong></td>
  <td><%= @github_profile.name %></tr></td>
</tr>
<tr>
  <td><strong>Location:</strong></td>
  <td><%= @github_profile.location %></td>
</tr>
<tr>
  <td><strong>Public repo:</strong></td>
  <td><%= @github_profile.public_repo %></td>
</tr>
<tr>
  <td><strong>Public gists:</strong></td>
  <td><%= @github_profile.public_gists %></td>
</tr>
<tr>
  <td><strong>Followers:</strong></td>
  <td><%= @github_profile.followers %></td>
</tr>
<tr>
  <td><strong>Following:</strong></td>
  <td><%= @github_profile.following %></td>
</tr>
<tr>
  <td><strong>Member since:</strong></td>
  <td><%= @github_profile.member_since %></td>
</tr>
<tr>
  <td><strong>Private repos:</strong></td>
  <td><%= @github_profile.private_repos %></td>
</tr>
<tr>
  <td><strong>Github link:</strong></td>
  <td><%= @github_profile.github_link %></td>
</tr>
<tr>
  <td><strong>Blog link:</strong></td>
  <td><%= @github_profile.blog_link %></td>
</tr>
<tr>
  <td><strong>Bio:</strong></td>
  <td><%= @github_profile.bio %></td>
</tr>

</table>
<hr>
<%= link_to 'Edit', edit_github_profile_path(@github_profile), class: 'btn btn-xs btn-white' %>
<%= link_to 'Back', welcome_profile_path, class: 'btn btn-xs btn-white' %>
</div>
</div>
</div>
</div>